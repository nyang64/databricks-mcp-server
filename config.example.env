# Databricks Configuration
# Copy this file to .env and fill in your actual values

# Databricks workspace URL (e.g., https://your-workspace.cloud.databricks.com)
DATABRICKS_HOST=https://your-workspace.cloud.databricks.com

# Personal Access Token from Databricks
# Generate this in User Settings > Developer > Access Tokens
DATABRICKS_TOKEN=your_personal_access_token_here

# Optional: Workspace ID (usually not needed for personal access tokens)
DATABRICKS_WORKSPACE_ID=your_workspace_id_here

